{
  "name": "apollo3",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "🛠 Development": "",
    "dev": "npm run watch",
    "watch": "npm run clear && concurrently -k npm:watch-codegen npm:watch-nextjs",
    "watch-nextjs": "next",
    "watch-codegen": "graphql-codegen --watch",
    "🏗 Build": "",
    "build": "npm run build-codegen && npm run build-nextjs",
    "build-nextjs": "next build",
    "build-codegen": "graphql-codegen",
    "✨ Utilities": "",
    "lint": "eslint 'src/**/*.{js,ts,tsx,graphql}'",
    "update-schema": "graphql-codegen -c ./codegen.download.yml",
    "clear": "find ./src -name \"__generated__\" -exec rm -rf '{}' +",
    "🏎 Runtime": "",
    "start": "next start"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@ant-design/icons": "4.7.0",
    "@apollo/client": "3.4.16",
    "@apollo/link-ws": "2.0.0-beta.3",
    "@apollo/react-ssr": "4.0.0",
    "antd": "4.16.13",
    "graphql": "16.0.0",
    "isomorphic-unfetch": "^3.0.0",
    "next": "12.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "subscriptions-transport-ws": "^0.9.16"
  },
  "devDependencies": {
    "@graphql-codegen/add": "3.1.0",
    "@graphql-codegen/cli": "2.2.2",
    "@graphql-codegen/near-operation-file-preset": "2.2.0",
    "@graphql-codegen/schema-ast": "2.3.0",
    "@graphql-codegen/typescript": "2.3.0",
    "@graphql-codegen/typescript-operations": "2.2.0",
    "@graphql-codegen/typescript-react-apollo": "3.2.0",
    "@graphql-eslint/eslint-plugin": "2.3.1",
    "@types/node": "16.11.6",
    "@types/react": "17.0.33",
    "@types/react-dom": "17.0.10",
    "@typescript-eslint/eslint-plugin": "5.2.0",
    "@typescript-eslint/parser": "5.2.0",
    "concurrently": "6.3.0",
    "eslint": "8.1.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.26.1",
    "eslint-plugin-react-hooks": "4.2.0",
    "npm-check": "5.9.2",
    "prettier": "2.4.1",
    "sass": "1.43.4",
    "typescript": "4.4.4"
  }
}
